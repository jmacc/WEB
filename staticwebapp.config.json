{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ],
            "allowedRoles": [
                "anonymous"
            ]
        }
    ]
}